<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pramod.zillow" >
    <uses-sdk android:minSdkVersion="11"
        android:targetSdkVersion="21"/>
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/zillow"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="pramod.zillow.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="pramod.zillow.ResultActivity"
            android:label="@string/title_activity_result" >
        </activity>
        <activity
            android:name="pramod.zillow.BasicInfo"
            android:label="@string/title_activity_basic_info" >
        </activity>
        <activity
            android:name="pramod.zillow.Charts"
            android:label="@string/title_activity_charts" >
        </activity>
    </application>

</manifest>
